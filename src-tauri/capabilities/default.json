{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "os:default",
    "fs:default",
    "sql:default", 
    "sql:allow-load", 
    "sql:allow-execute", 
    "sql:allow-close",
    "store:default",
    {
      "identifier": "fs:allow-exists",
      "allow": [{ "path": "$APPDATA/*" }]
    },
    "updater:default",
    "updater:allow-check"
  ]
}